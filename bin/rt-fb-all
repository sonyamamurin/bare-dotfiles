#!/usr/bin/env sh

## Script to run filebot in rtorrent data folder using exclude file to catch
## files missed by rt post execution script.

$HOME/filebot/filebot.sh -script fn:amc "$HOME/rtorrent/data" --output "/mnt/vind/plex" --log-file "$HOME/filebot/logs/rt-fb-all.log" --action duplicate --conflict auto -non-strict --def unsorted=y artwork=y subtitles=en,sv skipExtract=y excludeList=amc.excludes plex=172.0.0.1:$PLEX_X_TOKEN
